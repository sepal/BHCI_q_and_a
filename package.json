{
  "name": "BHCI_q_and_a",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-js": "browserify ./src/js/app.js -t babelify --outfile ./dist/js/app.js",
    "watch-js": "nodemon -e js -w ./src/js -x 'npm run build-js'",
    "build-sass": "node-sass ./src/sass/styles.scss | postcss -u autoprefixer -o ./dist/css/styles.css",
    "watch-sass": "nodemon -e scss -w ./src/sass -x 'npm run build-sass'",
    "watch": "npm run watch-js & npm run watch-sass"
  },
  "dependencies": {
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "babelify": "^6.1.2",
    "bootstrap-sass": "^3.3.5",
    "browserify": "^10.2.4",
    "jquery": "^2.1.4",
    "node-sass": "^3.2.0",
    "postcss-cli": "^1.3.1",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.5",
    "rx": "^2.5.3",
    "rx-react": "^0.2.0"
  }
}
